<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <navigation-rule>
  <from-view-id>*</from-view-id>
    <navigation-case>
   <from-outcome>anasayfa</from-outcome>
   <to-view-id>/view/anasayfa.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>generalparam</from-outcome>
   <to-view-id>/view/generalparam.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>invoice</from-outcome>
   <to-view-id>/view/invoice.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>analysis</from-outcome>
   <to-view-id>/analysis.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>envelope</from-outcome>
   <to-view-id>/view/envelope.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>report</from-outcome>
   <to-view-id>/view/report.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>logger</from-outcome>
   <to-view-id>/view/logs.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>AdminLogger</from-outcome>
   <to-view-id>/webAdmin/admin.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>customers</from-outcome>
   <to-view-id>/view/customers.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>customerinvoiceid</from-outcome>
   <to-view-id>/view/customerinvoiceid.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>loginSuccess</from-outcome>
   <to-view-id>/view/sample.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login/adminLogin.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>admLoginSuccess</from-outcome>
   <to-view-id>/webAdmin/admin.jsf</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>
